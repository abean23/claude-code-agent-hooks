{
  "target": "division:divide",
  "cases": [
    {
      "id": "divide_by_zero",
      "call": {"args": [1.0, 0.0]},
      "expectation": {"raises": {"types": ["ZeroDivisionError"]}}
    },
    {
      "id": "divide_zero_by_zero",
      "call": {"args": [0.0, 0.0]},
      "expectation": {"raises": {"types": ["ZeroDivisionError"]}}
    },
    {
      "id": "divide_positive_infinity_by_zero",
      "call": {"args": ["Infinity", 0.0]},
      "expectation": {"raises": {"types": ["ZeroDivisionError"]}}
    },
    {
      "id": "divide_negative_infinity_by_zero",
      "call": {"args": ["-Infinity", 0.0]},
      "expectation": {"raises": {"types": ["ZeroDivisionError"]}}
    },
    {
      "id": "divide_by_infinity",
      "call": {"args": [1.0, "Infinity"]},
      "expectation": {"equals": {"value": 0.0, "tolerance": 1e-10}}
    },
    {
      "id": "divide_by_negative_infinity",
      "call": {"args": [1.0, "-Infinity"]},
      "expectation": {"equals": {"value": 0.0, "tolerance": 1e-10}}
    },
    {
      "id": "divide_infinity_by_positive",
      "call": {"args": ["Infinity", 1.0]},
      "expectation": {"predicate": {"name": "math.isinf"}}
    },
    {
      "id": "divide_negative_infinity_by_positive",
      "call": {"args": ["-Infinity", 1.0]},
      "expectation": {"predicate": {"name": "math.isinf"}}
    },
    {
      "id": "divide_infinity_by_infinity",
      "call": {"args": ["Infinity", "Infinity"]},
      "expectation": {"predicate": {"name": "math.isnan"}}
    },
    {
      "id": "divide_infinity_by_negative_infinity",
      "call": {"args": ["Infinity", "-Infinity"]},
      "expectation": {"predicate": {"name": "math.isnan"}}
    },
    {
      "id": "divide_nan_by_number",
      "call": {"args": ["NaN", 1.0]},
      "expectation": {"predicate": {"name": "math.isnan"}}
    },
    {
      "id": "divide_number_by_nan",
      "call": {"args": [1.0, "NaN"]},
      "expectation": {"predicate": {"name": "math.isnan"}}
    },
    {
      "id": "divide_nan_by_nan",
      "call": {"args": ["NaN", "NaN"]},
      "expectation": {"predicate": {"name": "math.isnan"}}
    },
    {
      "id": "divide_very_small_by_very_large",
      "call": {"args": [1e-308, 1e308]},
      "expectation": {"equals": {"value": 0.0, "tolerance": 1e-10}}
    },
    {
      "id": "divide_very_large_by_very_small",
      "call": {"args": [1e308, 1e-308]},
      "expectation": {"predicate": {"name": "math.isinf"}}
    },
    {
      "id": "divide_negative_by_positive",
      "call": {"args": [-10.0, 2.0]},
      "expectation": {"equals": {"value": -5.0, "tolerance": 1e-10}}
    },
    {
      "id": "divide_positive_by_negative",
      "call": {"args": [10.0, -2.0]},
      "expectation": {"equals": {"value": -5.0, "tolerance": 1e-10}}
    },
    {
      "id": "divide_negative_by_negative",
      "call": {"args": [-10.0, -2.0]},
      "expectation": {"equals": {"value": 5.0, "tolerance": 1e-10}}
    }
  ]
}